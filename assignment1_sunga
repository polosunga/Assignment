#include <iostream>

using namespace std;

int main()
{
	int month,day,year;
	
	cout << "Enter Month: ";
	cin >> month;
	
	cout << "Enter Day: ";
	cin >> day;
	
	cout << "Enter Year: ";
	cin >> year;
	
 	switch (month)
  		 {
 		    case 1:
  		 		if (day<0)
  		 		   cout << "Invalid Date!";
  		 		else if (day>31)
				   cout << "Invalid Date!";   
  		 		else   
				   cout << "January " << day << ", " << year;
  		 		break;
				   	
            case 2:
	 	        if (day<0)
  		 		   cout << "Invalid Date!";
  		 		else if (day==29)
	  			   if (year %4 == 0)
					 cout << "February " << day << ", " << year;
				   else 
				     cout << "Invalid Date!"; 	
				else if (day>28)
				 	 cout << "Invalid Date!";
				else          	
				   cout << "February " << day << ", " << year;
			    break;
				 
	 		case 3:
	 	        if (day<0)
  		 		   cout << "Invalid Date!";
  		 		else if (day>31)
	  			   cout << "Invalid Date!";	
				else    
				   cout << "March" << day << ", " << year;
		   		break;
		   		
  			case 4:
	 	        if (day<0)
  		 		   cout << "Invalid Date!";
  		 		else if (day>30)
	  			   cout << "Invalid Date!";	
				else      				
		   		cout << "April" << day << ", " << year;
		   		break;
				   	
		   	case 5:
	 	        if (day<0)
  		 		   cout << "Invalid Date!";
  		 		else if (day>31)
	  			   cout << "Invalid Date!";	
				else   		   		
		   		cout << "May" << day << ", " << year;
		   		break;	
		   		
		   	case 6:
	 	        if (day<0)
  		 		   cout << "Invalid Date!";
  		 		else if (day<30)
	  			   cout << "Invalid Date!";	
				else		   		
		   		cout << "June" << day << ", " << year;
		   		break;
				   		   
			case 7:
	 	        if (day<0)
  		 		   cout << "Invalid Date!";
  		 		else if (day>31)
	  			   cout << "Invalid Date!";	
				else   		   					
		   		cout << "July" << day << ", " << year;
		   		break;
			
			case 8:
	 	        if (day<0)
  		 		   cout << "Invalid Date!";
  		 		else if (day>31)
	  			   cout << "Invalid Date!";	
				else   		   					
		   		cout << "August" << day << ", " << year;
		   		break;
				   
			case 9:
	 	        if (day<0)
  		 		   cout << "Invalid Date!";
  		 		else if (day>30)
	  			   cout << "Invalid Date!";	
				else				
		   		cout << "September" << day << ", " << year;
		   		break;	   	   	  
				   
			case 10:
	 	        if (day<0)
  		 		   cout << "Invalid Date!";
  		 		else if (day>31)
	  			   cout << "Invalid Date!";	
				else   		   					
		   		cout << "October" << day << ", " << year;
		   		break;
			
			case 11:
	 	        if (day<0)
  		 		   cout << "Invalid Date!";
  		 		else if (day>30)
	  			   cout << "Invalid Date!";	
				else				
		   		cout << "November" << day << ", " << year;
		   		break;
				   
			case 12:
	 	        if (day<0)
  		 		   cout << "Invalid Date!";
  		 		else if (day>31)
	  			   cout << "Invalid Date!";	
				else   		   					
		   		cout << "December" << day << ", " << year;
		   		break;
				   
			default:
				cout << "Invalid date!";
				break;	   	   	   	    	
		   }   
	 
	return 0;
}
